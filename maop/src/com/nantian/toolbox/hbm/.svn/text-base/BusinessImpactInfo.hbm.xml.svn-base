<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.nantian.toolbox.vo">
	<class dynamic-insert="true" dynamic-update="true" name="BusinessImpactInfoVo"
		table="BUSINESS_IMPACT_INFO">
		<id name="id" column="ID" type="java.lang.Integer">
		  	<generator class="sequence">
		  		<param name="sequence">BUSINESS_IMPACT_INFO_SEQ</param>
		  	</generator>
		</id>
		<property generated="never" lazy="false" name="appsys_code"
			 type="java.lang.String">
			<column name="APPSYS_CODE">
			</column>
		</property>
		<property generated="never" lazy="false" name="business_impact_type"
			 type="java.lang.String">
			<column name="BUSINESS_IMPACT_TYPE">
			</column>
		</property>
		<property generated="never" lazy="false" name="business_impact_level"
			 type="java.lang.String">
			<column name="BUSINESS_IMPACT_LEVEL">
			</column>
		</property>
		<property generated="never" lazy="false" name="business_impact_content"
			 type="java.lang.String">
			<column name="BUSINESS_IMPACT_CONTENT">
			</column>
		</property>
	</class>
</hibernate-mapping>
