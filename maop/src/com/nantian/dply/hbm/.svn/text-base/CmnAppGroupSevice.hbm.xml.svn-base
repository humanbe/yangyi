<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.nantian.dply.vo">
	<class name="CmnAppGroupServiceVo" table="CMN_APP_GROUP_SERVICE" 
	dynamic-update="true" dynamic-insert="true">
		<comment>table</comment>
		<composite-id>
		<key-property name="appsysCode" type="java.lang.String">
			<column name="APPSYS_CODE">
				<comment>系统代码</comment>
			</column>
		</key-property>
		<key-property name="serviceName" type="java.lang.String">
			<column name="SERVICE_NAME">
				<comment>服务名称</comment>
			</column>
		</key-property>
	</composite-id>
		<property name="serverGroup" type="java.lang.String">
				<column name="SERVER_GROUP">
					<comment>服务器分组</comment>
				</column>
			</property>
			<property name="nfsFlag" type="java.lang.String">
				<column name="NFS_FLAG">
					<comment>NFS标示</comment>
				</column>
			</property>
			<property name="osType" type="java.lang.String">
				<column name="OS_TYPE">
					<comment>操作系统类型</comment>
				</column>
			</property>
			<property name="environmentCode" type="java.lang.String">
				<column name="ENVIRONMENT_CODE">
					<comment>操作系统类型</comment>
				</column>
			</property>
	</class>
</hibernate-mapping>

