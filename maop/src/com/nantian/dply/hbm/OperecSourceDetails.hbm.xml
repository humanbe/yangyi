<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.nantian.dply.vo">
 <class dynamic-insert="true" dynamic-update="true"
  name="OperecSourceDetailsVo" table="DPLY_OPEREC_SOURCE_DETAILS">
  <composite-id>
   <key-property name="functionId" type="java.lang.String">
    <column name="FUNCTION_ID" not-null="true">
    	<comment>功能ID</comment>
    </column>
   </key-property>
   <key-property name="execYmdhmsf" type="java.lang.String">
    <column name="EXEC_YMDHMSF" not-null="true">
    	<comment>执行年月日时分毫秒</comment>
    </column>
   </key-property>
   <key-property name="procomInputFile" type="java.lang.String">
    <column name="PROCOM_INPUT_FILE" not-null="true">
    	<comment>处理完成输入文件</comment>
    </column>
   </key-property>
   <key-property name="seqNo" type="java.lang.Integer">
    <column name="SEQ_NO" not-null="true">
    	<comment>序列号</comment>
    </column>
   </key-property>
  </composite-id>
  <property name="appsysCode" generated="never" lazy="false" not-null="true" type="java.lang.String">
   <column name="APPSYS_CODE">
       	<comment>应用系统编号</comment>
   </column>
  </property>
  <property name="subAppsysCode" generated="never" lazy="false" not-null="true" type="java.lang.String">
   <column name="SUB_APPSYS_CODE">
       	<comment>子系统编号</comment>
   </column>
  </property>
  <property name="resourceId" generated="never" lazy="false" not-null="true" type="java.lang.String">
   <column name="RESOURCE_ID">
       	<comment>资源ID</comment>
   </column>
  </property>
  <property name="resourcePath" generated="never" lazy="false" not-null="true" type="java.lang.String">
   <column name="RESOURCE_PATH">
       	<comment>资源路径</comment>
   </column>
  </property>
  <property name="recordMessage" generated="never" lazy="false" type="java.lang.String">
   <column name="RECORD_MESSAGE">
       	<comment>记录信息</comment>
   </column>
  </property>
  <property name="execMessage" generated="never" lazy="false" not-null="true" type="java.lang.String">
   <column name="EXEC_MESSAGE">
       	<comment>执行结果状态</comment>
   </column>
  </property>
  <property name="execResultStatus" generated="never" lazy="false" type="java.lang.String">
   <column name="EXEC_RESULT_STATUS">
       	<comment>执行信息</comment>
   </column>
  </property>
  <property generated="never" lazy="false" name="reserve1" type="java.lang.String">
   <column name="RESERVE1">
       	<comment>备用1</comment>
   </column>
  </property>
  <property generated="never" lazy="false" name="reserve2" type="java.lang.String">
   <column name="RESERVE2">
       	<comment>备用2</comment>
   </column>
  </property>
  <property generated="never" lazy="false" name="reserve3" type="java.lang.String">
   <column name="RESERVE3">
       	<comment>备用3</comment>
   </column>
  </property>
  <property generated="never" lazy="false" name="reserve4" type="java.lang.String">
   <column name="RESERVE4">
       	<comment>备用4</comment>
   </column>
  </property>
  <property generated="never" lazy="false" name="reserve5" type="java.lang.String">
   <column name="RESERVE5">
       	<comment>备用5</comment>
   </column>
  </property>
  <property generated="never" lazy="false" name="deleteFlag" type="java.lang.String">
   <column name="DELETE_FLAG">
       	<comment>删除标示</comment>
   </column>
  </property>
 </class>
</hibernate-mapping>
