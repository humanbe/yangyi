<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.nantian.check.vo">
	<class name="CheckItemNshInfoVo" table="CHECK_ITEM_NSH_INFO" dynamic-update="true"
		dynamic-insert="true">
    <composite-id>
		<key-property name="checkItemCode" type="java.lang.String">
			<column name="CHECK_ITEM_CODE">
				<comment>系统代码</comment>
			</column>
		</key-property>
	</composite-id>
		<property name="itemScriptName" type="java.lang.String">
			<column name="ITEM_SCRIPT_NAME">
			</column>
		</property>
		<property name="itemScriptPath" type="java.lang.String">
			<column name="ITEM_SCRIPT_PATH">
			</column>
		</property>
	</class>
</hibernate-mapping>


