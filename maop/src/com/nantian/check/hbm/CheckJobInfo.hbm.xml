		<property name="language_type" type="java.lang.String">
			<column name="LANGUAGE_TYPE">
			</column>
		</property>
		<property name="server_target_type" type="java.lang.String">
			<column name="SERVER_TARGET_TYPE">
			</column>
		</property>
	</class>
</hibernate-mapping>


