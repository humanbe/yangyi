<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.nantian.jedarpt.vo">
	<class name="JedaColumnVo" table="JEDA_COLUMN" dynamic-update="true"
		dynamic-insert="true">
        <id name="column_code" column="COLUMN_CODE" type="integer">
		  	<generator class="sequence">
		  		<param name="sequence">JEDA_COLUMN_SEQ</param>
		  	</generator>
		</id>
		
		<property name="column_en_name" type="string">
			<column name="COLUMN_EN_NAME"></column>
		</property>
		<property name="column_ch_name" type="string">
			<column name="COLUMN_CH_NAME"></column>
		</property>
		<property name="column_desc" type="string">
			<column name="COLUMN_DESC"></column>
		</property>
		<property name="delete_flag" type="string">
			<column name="DELETE_FLAG"></column>
		</property>
		<property name="is_trans" type="string">
			<column name="IS_TRANS"></column>
		</property>
		<property name="dic_code" type="string">
			<column name="DIC_CODE"></column>
		</property>
	</class>
</hibernate-mapping>


