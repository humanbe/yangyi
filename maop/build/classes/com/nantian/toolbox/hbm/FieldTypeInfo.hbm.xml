<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.nantian.toolbox.vo">
	<class dynamic-insert="true" dynamic-update="true" name="FieldTypeInfoVo"
		table="FIELD_TYPE_INFO">
		
		<id name="field_id" column="FIELD_ID" type="java.lang.Integer">
		  	<generator class="sequence">
		  		<param name="sequence">FIELD_TYPE_INFO_SEQ</param>
		  	</generator>
		</id>
		<property generated="never" lazy="false" name="field_type_direction"
			 type="java.lang.String">
			<column name="FIELD_TYPE_DIRECTION">
			</column>
		</property>
		<property generated="never" lazy="false" name="field_type_one"
			not-null="true" type="java.lang.String">
			<column name="FIELD_TYPE_ONE">
			</column>
		</property>
		<property generated="never" lazy="false" name="field_type_two"
			 type="java.lang.String">
			<column name="FIELD_TYPE_TWO">
			</column>
		</property>
		<property generated="never" lazy="false" name="field_type_three"
			 type="java.lang.String">
			<column name="FIELD_TYPE_THREE">
			</column>
		</property>
		<property generated="never" lazy="false" name="comments"
			 type="java.lang.String">
			<column name="COMMENTS">
			</column>
		</property>
		<property generated="never" lazy="false" name="filed_creator"
			 type="java.lang.String">
			<column name="FILED_CREATOR">
			</column>
		</property>
		<property generated="never" lazy="false" name="filed_created"
			 type="java.sql.Timestamp">
			<column name="FILED_CREATED">
			</column>
		</property>
		<property generated="never" lazy="false" name="filed_modifier"
			type="java.lang.String">
			<column name="FILED_MODIFIER">
			</column>
		</property>
		<property generated="never" lazy="false" name="filed_modified"
			type="java.sql.Timestamp">
			<column name="FILED_MODIFIED">
			</column>
		</property>

	</class>
</hibernate-mapping>
