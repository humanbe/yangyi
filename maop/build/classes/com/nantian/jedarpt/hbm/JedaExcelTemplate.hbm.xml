<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.nantian.jedarpt.vo">
	<class name="JedaExcelTemplateVo" table="JEDA_EXCEL_TEMPLATE" dynamic-update="true"
		dynamic-insert="true">
        <id name="template_code" column="TEMPLATE_CODE" type="integer">
		  	<generator class="sequence">
		  		<param name="sequence">JEDA_TEMPLATE_SEQ</param>
		  	</generator>
		</id>
		
		<property name="report_code" type="integer">
			<column name="REPORT_CODE"></column>
		</property>
		<property name="template_en_name" type="string">
			<column name="TEMPLATE_EN_NAME"></column>
		</property>
		<property name="template_ch_name" type="string">
			<column name="TEMPLATE_CH_NAME"></column>
		</property>
		<property name="start_row_num" type="string">
			<column name="START_ROW_NUM"></column>
		</property>
		<property name="start_col_num" type="string">
			<column name="START_COL_NUM"></column>
		</property>
		<property name="delete_flag" type="string">
			<column name="DELETE_FLAG"></column>
		</property>
	</class>
</hibernate-mapping>


