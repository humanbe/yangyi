package com.nantian.component.ftp;

/**
 * FTP常量Class
 * @author dong
 */
public class ComponentFtpConst {

	/** errorCode:连接失败 */
	public static final int ERROR_CONNECT = 30000001;

	/** errorCode:未知主机 */
	public static final int ERROR_UNKNOWN_HOST = 30000002;

	/** errorCode:返回错误的应答code */
	public static final int ERROR_REPLY_CODE = 30000003;

	/** errorCode:认证失败 */
	public static final int ERROR_AUTHENTICATION = 30000004;

	/** errorCode:参数错误 */
	public static final int ERROR_INVALID_PARAMETER = 30000005;

	/** 上传的文件不存在 */
	public static final int ERROR_UPLOAD_FILE_NOT_FOUND = 30000011;

	/** 无法上传文件 */
	public static final int ERROR_CANNOT_PUT = 30000012;

	/** 无法创建目录 */
	public static final int ERROR_CANNOT_MKDIR = 30000013;

	/** 无法下载文件 */
	public static final int ERROR_CANNOT_GET = 30000015;

	/** 指定的下载文件在本地无法创建 */
	public static final int ERROR_DOWNLOAD_FILE_CANNOT_CREATE = 30000016;
	
	/** errorCode:未连接 */
	public static final int ERROR_NOT_CONNECT = 30000018;

	/** errorCode:未知异常 */
	public static final int ERROR_UNKNOWN = 30009999;
