<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.nantian.dply.vo">
	<class name="CmnServerAgentApplyVo" table="CMN_SERVER_AGENT_APPLY" dynamic-update="true"
		dynamic-insert="true">
		<comment>table</comment>
		<composite-id>
		   <key-property name="recordId" type="java.lang.String">
		    <column name="RECORD_ID" not-null="true">
		    	<comment>参数名称</comment>
		    </column>
		   </key-property>
	  	</composite-id>
		 	<property name="appsysCode" type="java.lang.String">
			<column name="APPSYS_CODE">
				<comment>重要标识</comment>
			</column>
		</property>
		<property name="serverIp" type="java.lang.String">
			<column name="SERVER_IP">
				<comment>重要标识</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>


