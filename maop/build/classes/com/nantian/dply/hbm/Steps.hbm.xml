<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.nantian.dply.vo">
	<class name="StepsVo" table="STEPS" dynamic-update="true"
		dynamic-insert="true">
        <id name="id" column="ID" type="java.lang.Long">
		  	<generator class="sequence">
		  		<param name="sequence">STEPS_SEQ</param>
		  	</generator>
		</id>
		
		<property name="position" type="java.lang.Long">
			<column name="POSITION">
			</column>
		</property>
		<property name="requestId" type="java.lang.Long">
			<column name="REQUEST_ID">
			</column>
		</property>
		<property name="name" type="java.lang.String">
			<column name="NAME">
			</column>
		</property>
		<property name="differentLevelFromPrevious" type="java.lang.Long">
			<column name="DIFFERENT_LEVEL_FROM_PREVIOUS">
			</column>
		</property>
		<property name="serviceCode" type="java.lang.String">
			<column name="SERVICE_CODE">
			</column>
		</property>
		<property name="servers" type="java.lang.String">
			<column name="SERVERS">
			</column>
		</property>
		<property name="component" type="java.lang.String">
			<column name="COMPONENT">
			</column>
		</property>
		<property name="manual" type="java.lang.Long">
			<column name="MANUAL">
			</column>
		</property>
		<property name="owner" type="java.lang.String">
			<column name="OWNER">
			</column>
		</property>
		<property name="autoJobName" type="java.lang.String">
			<column name="AUTO_JOB_NAME">
			</column>
		</property>
		<property name="createStatus" type="java.lang.String">
			<column name="CREATE_STATUS">
			</column>
		</property>
		<property name="operateLog" type="java.lang.String">
			<column name="OPERATE_LOG">
			</column>
		</property>
		<property name="createdAt" type="java.sql.Timestamp">
			<column name="CREATED_AT">
			</column>
		</property>
		
		<property name="updatedAt" type="java.sql.Timestamp">
			<column name="UPDATED_AT">
			</column>
		</property>
		
	</class>
</hibernate-mapping>


