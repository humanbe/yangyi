<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.nantian.common.system.vo">
	<class name="AppInfoVo" table="CMN_APP_INFO" dynamic-update="true" dynamic-insert="true">
		<comment>应用系统信息</comment>
		<id name="appsysCode" type="java.lang.String">
			<column name="APPSYS_CODE">
				<comment>系统编号</comment>
			</column>
		</id>
		<property name="appsysName" type="java.lang.String">
			<column name="APPSYS_NAME">
				<comment>系统名称</comment>
			</column>
		</property>
		<property name="sysA" type="java.lang.String">
			<column name="SYSA">
				<comment>系统管理员A</comment>
			</column>
		</property>
		<property name="sysB" type="java.lang.String">
			<column name="SYSB">
				<comment>系统管理员B</comment>
			</column>
		</property>
		<property name="appA" type="java.lang.String"> 
			<column name="APPA">
				<comment>应用管理员A</comment>
			</column>
		</property>
		<property name="appB" type="java.lang.String">
			<column name="APPB">
				<comment>应用管理员B</comment>
			</column>
		</property>
		<property name="projectLeader" type="java.lang.String">
			<column name="DEVELOP">
				<comment>项目组负责人</comment>
			</column>
		</property>
		<property name="sysStatus" type="java.lang.String">
			<column name="FLAG">
				<comment>系统状态. (未下线;使用中;已下线;未上线)</comment>
			</column>
		</property>
		<property name="department" type="java.lang.String">
			<column name="DEPART">
				<comment>业务主管部门(多部门以逗号分隔)</comment>
			</column>
		</property>
		<property name="serviceTime" type="java.lang.String">
			<column name="FUWU">
				<comment>服务级别</comment>
			</column>
		</property>
		<property name="disasterRecoverPriority" type="java.lang.String">
			<column name="ANQUAN">
				<comment>灾备优先级(A:最高 B:中等 C:最低)</comment>
			</column>
		</property>
		<property name="securityRank" type="java.lang.String">
			<column name="JIBIE">
				<comment>安全定级,1-4(4为最高，以此类推)</comment>
			</column>
		</property>
		<property name="operationsManager" type="java.lang.String">
			<column name="MANAGER">
				<comment>运维经理</comment>
			</column>
		</property>
		<property name="appOutline" type="java.lang.String">
			<column name="APP_OUTLINE">
				<comment>系统概要</comment>
			</column>
		</property>
		<property name="appType" type="java.lang.String">
			<column name="APP_TYPE">
				<comment>系统类型(1:平稳形、2:震荡形)</comment>
			</column>
		</property>
		
		<property name="hexinJibie" type="java.lang.String">
			<column name="HEXIN_JIBIE">
				<comment>核心级别A:最高 B:中等 C:最低</comment>
			</column>
		</property>
		<property name="zhongyaoJibie" type="java.lang.String">
			<column name="ZHONGYAO_JIBIE">
			<comment>重要A:最高 B:中等 C:最低</comment>
			</column>
		</property>
		<property name="guanjianJibie" type="java.lang.String">
			<column name="GUANJIAN_JIBIE">
			<comment>关键A:最高 B:中等 C:最低</comment>
			</column>
		</property>
		<property name="groupName" type="java.lang.String">
			<column name="GROUPNAME">
				<comment>团队组别</comment>
			</column>
		</property>
		<property name="aopFlag" type="java.lang.String">
			<column name="AOP_FLAG">
				<comment>运维自动化平台上线标示</comment>
			</column>
		</property>
		<property name="lastScanTime" type="java.sql.Timestamp">
			<column name="LAST_SCAN_TIME">
				<comment>上次扫描时间</comment>
			</column>
		</property>
		<property name="deleteFlag" type="java.lang.String">
			<column name="DELETE_FLAG">
				<comment>删除标示</comment>
			</column>
		</property>
		<property name="updateTime" type="java.sql.Timestamp">
			<column name="UPDATE_TIME">
				<comment>更新时间</comment>
			</column>
		</property>
		<property name="updateBeforeRecord" type="java.lang.String">
			<column name="UPDATE_BEFORE_RECORD">
				<comment>更新前纪录</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>


