<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.nantian.rept.vo">
 <class dynamic-insert="true" dynamic-update="true"
  name="SysResrcTransVo" table="SYS_RESRC_TRANS">
  <composite-id>
   <key-property name="aplCode" type="java.lang.String">
    <column name="APL_CODE" not-null="true">
    	<comment>应用系统编号</comment>
    </column>
   </key-property>
    <key-property name="tranName" type="java.lang.String">
    <column name="TRAN_NAME" not-null="true">
    	<comment>交易名称</comment>
    </column>
   </key-property>
   <key-property name="srvType" type="java.lang.String">
    <column name="SRV_TYPE" not-null="true">
    	<comment>服务器分类</comment>
    </column>
   </key-property>
   <key-property name="srvCode" type="java.lang.String">
    <column name="SRV_CODE" not-null="true">
    	<comment>服务器编号（IP）</comment>
    </column>
   </key-property>
  </composite-id>
  </class>
</hibernate-mapping>
